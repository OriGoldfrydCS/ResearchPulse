{
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python"
        },
        {
            "src": "static/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "static/$1",
            "headers": {
                "Cache-Control": "public, max-age=0, must-revalidate"
            }
        },
        {
            "src": "/(.*)",
            "dest": "main.py"
        }
    ]
}